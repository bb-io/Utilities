<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <TargetFramework>net8.0</TargetFramework>
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>
    <Product>Utilities</Product>
    <Description>Various utility actions to parse or convert data</Description>
    <Version>1.1.2</Version>
    <AssemblyName>Apps.Utilities</AssemblyName>
  </PropertyGroup>
  <ItemGroup>
    <None Remove="image\icon.png" />
  </ItemGroup>
  <ItemGroup>
    <EmbeddedResource CopyToOutputDirectory="Always" Include="image\icon.png" />
  </ItemGroup>
  <ItemGroup>
    <PackageReference Include="Blackbird.Applications.Sdk.Common" Version="2.3.2-alpha1" />
  </ItemGroup>
</Project>